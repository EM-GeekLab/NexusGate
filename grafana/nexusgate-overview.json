{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "10.0.0"
    },
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus",
      "version": "1.0.0"
    },
    {
      "type": "panel",
      "id": "timeseries",
      "name": "Time series",
      "version": ""
    },
    {
      "type": "panel",
      "id": "piechart",
      "name": "Pie chart",
      "version": ""
    },
    {
      "type": "panel",
      "id": "stat",
      "name": "Stat",
      "version": ""
    },
    {
      "type": "panel",
      "id": "gauge",
      "name": "Gauge",
      "version": ""
    }
  ],
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Total Completions",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(nexusgate_completions_total)",
          "legendFormat": "Total",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Total Embeddings",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(nexusgate_embeddings_total)",
          "legendFormat": "Total",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Active API Keys",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "purple", "value": null }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "nexusgate_active_api_keys",
          "legendFormat": "Keys",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Success Rate",
      "type": "gauge",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 0.9 },
              { "color": "green", "value": 0.95 }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(nexusgate_completions_total{status=\"completed\"}) / clamp_min(sum(nexusgate_completions_total), 1)",
          "legendFormat": "Success Rate",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Request Trend",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 16, "x": 0, "y": 4 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10,
            "pointSize": 5,
            "showPoints": "auto"
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "targets": [
        {
          "expr": "sum(rate(nexusgate_completions_total[5m])) by (status)",
          "legendFormat": "Completions ({{status}})",
          "refId": "A"
        },
        {
          "expr": "sum(rate(nexusgate_embeddings_total[5m])) by (status)",
          "legendFormat": "Embeddings ({{status}})",
          "refId": "B"
        }
      ]
    },
    {
      "title": "Model Distribution",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 4 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right" },
        "pieType": "donut",
        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "expr": "sum by (model)(nexusgate_completions_total)",
          "legendFormat": "{{model}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Token Usage",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10,
            "stacking": { "mode": "normal" }
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "targets": [
        {
          "expr": "sum(rate(nexusgate_tokens_prompt_total[5m]))",
          "legendFormat": "Prompt Tokens/s",
          "refId": "A"
        },
        {
          "expr": "sum(rate(nexusgate_tokens_completion_total[5m]))",
          "legendFormat": "Completion Tokens/s",
          "refId": "B"
        },
        {
          "expr": "sum(rate(nexusgate_tokens_embedding_total[5m]))",
          "legendFormat": "Embedding Tokens/s",
          "refId": "C"
        }
      ]
    },
    {
      "title": "Latency (P50 / P95 / P99)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 5
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(nexusgate_completion_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P50 Duration",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(nexusgate_completion_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P95 Duration",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(nexusgate_completion_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P99 Duration",
          "refId": "C"
        }
      ]
    },
    {
      "title": "Time to First Token (P50 / P95 / P99)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 20 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 5
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(nexusgate_completion_ttft_seconds_bucket[5m])) by (le))",
          "legendFormat": "P50 TTFT",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(nexusgate_completion_ttft_seconds_bucket[5m])) by (le))",
          "legendFormat": "P95 TTFT",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(nexusgate_completion_ttft_seconds_bucket[5m])) by (le))",
          "legendFormat": "P99 TTFT",
          "refId": "C"
        }
      ]
    },
    {
      "title": "Rate Limit Rejections",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 20 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 50
          },
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "targets": [
        {
          "expr": "sum(rate(nexusgate_rate_limit_rejections_total[5m])) by (type)",
          "legendFormat": "{{type}}",
          "refId": "A"
        }
      ]
    }
  ],
  "schemaVersion": 39,
  "tags": ["nexusgate", "llm", "ai"],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "",
  "title": "NexusGate Overview",
  "uid": "nexusgate-overview",
  "version": 1
}
